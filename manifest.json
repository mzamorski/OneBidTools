{
  "manifest_version": 3,
  "name": "XTB Tools",
  "description": "-",
  "version": "1.0",
  "action": {
    "default_title": "Settings",
    "default_popup": "settings.html"
  },
  "icons": { 
  },
  "content_scripts": [
    {
      "matches": [
        "*://xstation5.xtb.com/*"
      ],
      "js": [
        "content.js", "messageUtils.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }, 
  "permissions": [
    "storage", "tabs"
  ]
}